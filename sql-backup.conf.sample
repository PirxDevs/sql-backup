# Directory in which backup files will be stored
BACKUP_ROOT_DIR="/mnt/backup/sql"
# Directory in which PostgreSQL binaries are kept
# Macro %v will be replaced with PostgreSQL version
POSTGRESQL_BIN_DIR="/usr/lib/postgresql/%v/bin"
# Date format for backup folders, see "man date" for details
BACKUP_DATE_FORMAT="%Y%m%d"
# List of backup jobs to run
BACKUP_JOBS="example server1 server2"
